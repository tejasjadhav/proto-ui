{
  "name": "my-app",
  "version": "0.1.1",
  "author": "Your Name",
  "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "ace-builds": "^1.4.12",
    "bootstrap": "^4.5.3",
    "craco": "^0.0.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "grpc": "^1.24.4",
    "lodash": "^4.17.20",
    "protobufjs": "^6.10.2",
    "react": "^17.0.1",
    "react-ace": "^9.2.1",
    "react-bootstrap": "^1.4.0",
    "react-bootstrap-icons": "^1.2.2",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "typescript": "4.1.2",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:windev": "concurrently \"SET BROWSER=none && yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "eject": "react-scripts eject"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.167",
    "@types/react-redux": "^7.1.14",
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.0"
  }
}
